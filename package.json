{
  "name": "@paybin/sdk",
  "version": "1.0.0",
  "description": "Official Paybin SDK for Node.js and TypeScript - Easily integrate cryptocurrency payments",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "paybin",
    "cryptocurrency",
    "payment",
    "bitcoin",
    "ethereum",
    "crypto",
    "blockchain",
    "payment-gateway",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paybin/paybin-js.git"
  },
  "bugs": {
    "url": "https://github.com/paybin/paybin-js/issues"
  },
  "homepage": "https://github.com/paybin/paybin-js#readme"
}